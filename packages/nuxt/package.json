{
  "name": "nuxt-schema-org",
  "type": "module",
  "version": "0.0.1",
  "description": "Nuxt module for vueuse-schema-org",
  "keywords": [
    "vueuse-schema-org",
    "nuxt-module",
    "nuxt",
    "nuxt3"
  ],
  "license": "MIT",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/vueuse-schema-org.git",
    "directory": "integrations/nuxt"
  },
  "funding": "https://github.com/sponsors/harlan-zw",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/harlan-zw/vueuse-schema-org/integrations/nuxt#readme",
  "bugs": {
    "url": "https://github.com/harlan-zw/vueuse-schema-org/issues"
  },
  "scripts": {
    "build": "nuxi prepare ../../playgrounds/nuxt3 && nuxt-module-build",
    "prepack": "nuxt-module-build",
    "play": "nuxi dev ../../playgrounds/nuxt3",
    "play:build": "nuxi build ../../playgrounds/nuxt3",
    "stub": "nuxt-module-build --stub && nuxi prepare ../../playgrounds/nuxt3"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.1",
    "vueuse-schema-org": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "nuxt": "^3.0.0-rc.1"
  }
}
