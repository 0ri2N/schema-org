{
  "name": "nuxt-seo-kit",
  "type": "module",
  "version": "0.0.1",
  "description": "SEO utilities for Nuxt v3",
  "keywords": [
    "nuxt-module",
    "nuxt",
    "nuxt3",
    "seo"
  ],
  "homepage": "https://github.com/harlan-zw/nuxt-seo-kit#readme",
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-seo-kit/issues"
  },
  "license": "MIT",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-seo-kit.git"
  },
  "funding": "https://github.com/sponsors/harlan-zw",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/nuxi prepare playground && nuxt-module-build",
    "prepack": "../../node_modules/.bin/nuxt-module-build",
    "play": "../../node_modules/.bin/nuxi dev playground",
    "play:build": "../../node_modules/.bin/nuxi build playground",
    "stub": "../../node_modules/.bin/nuxt-module-build --stub && nuxi prepare playground"
  },
  "dependencies": {
    "@harlanzw/nuxt-kit-extras": "^0.3.1",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxt/schema": "npm:@nuxt/schema-edge@latest",
    "nuxt3": "^3.0.0-27465767.70f067a"
  }
}
