{
  "name": "@seo-meta-kit/monorepo",
  "private": true,
  "version": "0.3.5",
  "description": "Minimal, composable Expressive Universal Router",
  "repository": "harlan-zw/unrouted",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter ./packages run build",
    "stub": "pnpm -r --parallel --filter ./packages run stub && pnpm -r --parallel --filter ./integrations run stub",
    "lint": "eslint \"{packages,test,integrations}/**/*.{ts,vue,json,yml}\"",
    "lint:fix": "npm run lint -- --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@vitest/ui": "^0.9.3",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/composition-api": "^1.4.9",
    "@vue/test-utils": "^2.0.0-rc.19",
    "@vueuse/core": "^8.2.5",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "jsdom": "^19.0.0",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vitest": "^0.9.3",
    "vue": "^3.2.31",
    "vue2": "npm:vue@^2.6.14"
  },
  "pnpm": {
    "overrides": {
      "vue-demi": "0.12.5"
    }
  },
  "dependencies": {
    "@vueuse/shared": "^8.2.5",
    "vue-demi": "^0.12.5"
  }
}
